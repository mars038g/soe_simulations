journal = {ICES Journal of Marine Science},
keywords = {08002 barcelona,37-49,accepted 17 november 2009,advance access publication 7,and department of biology,blanchard,centre for environment,coll,community,dalhousie university,ecological indicators,ecosystem effects of fishing,ecosystem-based fisheries management,fisheries and aquaculture science,halifax,institute of marine,j,january 2010,l,lowestoft laboratory,m,natural resource management,received 11 june 2009,spain,suffolk nr33 0ht,tim de la barceloneta,uk},
number = {4},
pages = {732--744},
title = {{Trend analysis of indicators: a comparison of recent changes in the status of marine ecosystems around the world}},
url = {http://icesjms.oxfordjournals.org/content/early/2010/01/07/icesjms.fsp282{\%}5Cnhttp://icesjms.oxfordjournals.org/cgi/doi/10.1093/icesjms/fsp282},
volume = {67},
