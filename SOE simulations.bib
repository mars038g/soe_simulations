@article{Kulkarni1995,
abstract = {The effect of serial correlation on the performance of the Mann-Kendall test on the presence of a trend is examined by means of a Monte Carlo simulation. Even moderate serial correlation makes the test liberal so that it signals erroneously the presence of significant trends more often than permitted according to the significance level. For time series with an autocorrelation similar to that of an AR(1)-process a simple prewhitening procedure is proposed. The approach is demonstrated with a time series of annual mean sea-level air-pressure from Bombay},
